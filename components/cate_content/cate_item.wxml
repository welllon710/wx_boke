<navigator class="article_item" url="/pages/detail/detail?uid={{list.id}}&time={{time.msg}}" >
  <view class="left">
    <!-- <image src=""></image> -->
    <view class="img"></view>
  </view>
  <view class="right">
    <view class="r_title">
      {{list.title}}
